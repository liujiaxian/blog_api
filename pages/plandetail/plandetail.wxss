.art-header{
    position: relative;
    width: 100%;
    height: 219px;
    color: #FFF;
}

.art-img{
    width: 100%;
    height: 219px;
}

.art-title{
    position: absolute;
    bottom: 36px;
    left: 8px;
    font-size: 16px;
}

.art-source {
    position: absolute;
    bottom: 8px;
    font-size: 14px;
    right: 8px;   
}

.art-content{
    font-size: 14px;
    color: #616466;
    padding-left: 16px;
    padding-right: 16px;
    padding-top: 10px;
    background-color: #FFF;
    line-height: 1.5;
}

/**index.wxss**/
.container .search{
  width: 735px;
  height: 65px;
  padding: 12.5px 0 12.5px 15px;
  background: #2A8CE5;
}
.container .search-left{
  flex: 8;
  background: #4EA3E7;
  text-align: left;
}
.container .search-left input{
  display: inline-block;
  height: 65px;
  font-size: 26px;
}
.search-placeholder{
  color: #8CCEFD;
  line-height: 20px;
}
.container .search .search-left image{
  display: inline-block;
  width: 35px;
  height: 35px;
  padding: 15px 15px 15px 20px;
}
.container .search .search-right{
  flex: 1;
}
.container .search .search-right image{
  width: 45px;
  height: 45px;
  padding: 10px;
}

.container{
  padding: 0;
  font-size: 18px;
  background: #F0F4F3;
  color: #000;
}
/*feed-item part is in app.wxss for multiplexing*/

.navtitle{
  margin: 10px 0px;
  border-bottom:1px solid #8CCEFD;
}
.like{
  float:left;
}

.comments{
  float:right;
}

.clear-both{
  clear: both;
}

.answer-txt{
    line-height: 20px;
}
.plan-content{
  height:30px !important;
}
.plan-content-title{
  margin: 0px !important;
}